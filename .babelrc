{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@constants": "./src/constants",
          "@screens": "./src/screens",
          "@routes": "./src/routes",
          "@types": "./src/types",
          "@hooks": "./src/hooks",
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@helper": "./src/helper"
        }
      }
    ]
  ]
}
